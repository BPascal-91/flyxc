{
  "name": "flyxc",
  "version": "1.0.0",
  "description": "flyxc.app",
  "main": "server.js",
  "scripts": {
    "start": "node app/server.js",
    "build": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c --environment USE_CACHE,NODE_ENV:'production'",
    "debug": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -wc --environment USE_CACHE",
    "lint": "eslint 'frontend/src/**/*.ts' --quiet --fix && eslint 'app/src/**/*.ts' && eslint 'run/src/**/*.ts' --quiet --fix",
    "proto": "npx pbf track.proto > frontend/src/viewer/logic/track.js",
    "emulator:start": "gcloud beta emulators datastore start --data-dir=ds",
    "emulator:env": "gcloud beta emulators datastore env-init --data-dir=ds",
    "emulator:unenv": "gcloud beta emulators datastore env-unset --data-dir=ds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@google-cloud/datastore": "^4.4.1",
    "@google-cloud/storage": "^3.4.0",
    "@koa/multer": "^2.0.0",
    "@tmcw/togeojson": "^3.0.1",
    "geojson": "^0.5.0",
    "google-auth-library": "^5.5.1",
    "google-polyline": "^1.0.3",
    "gpx-builder": "^2.2.0",
    "igc-parser": "vicb/igc-parser#0.5.0",
    "ioredis": "^4.14.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "multer": "^1.4.2",
    "pbf": "^3.2.1",
    "printf": "^0.5.2",
    "qrcode": "^1.4.2",
    "request-zero": "^0.2.3",
    "simplify-path": "^1.1.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.0-dev.20191021",
    "xmlbuilder": "^13.0.2",
    "xmldom": "^0.1.27"
  },
